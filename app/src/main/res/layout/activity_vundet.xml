<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Vundet"
    android:background="@color/funkyColor1">


    <TextView
        android:id="@+id/Tillyke"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tillykke du har vundet"
        android:textColor="@color/funkyColor2"
        android:textSize="33dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.30"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/NytSpilVundet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Spil igen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Tillyke"
        app:layout_constraintVertical_bias="0.1"
        android:textColorHint="@color/colorPrimaryDark"/>


</androidx.constraintlayout.widget.ConstraintLayout>